{
  "name": "sisdai-componentes",
  "version": "3.0.1",
  "description": "Biblioteca de componentes de Vue reutilizables",
  "license": "LGPL-3.0-only",
  "main": "./src/index.js",
  "files": [
    "src",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf node_modules && rm package-lock.json && npm cache clean -f && npm cache verify && npm i",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "format": "prettier . --write",
    "format-lint": "npm run format && npm run lint",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "vue": "^3.4.30"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.3",
    "@testing-library/vue": "^8.1.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.5.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pinia": "^2.1.7",
    "prettier": "^3.3.2",
    "sisdai-css": "git+https://codigo.conahcyt.mx/sisdai/sisdai-css#v1.0.6",
    "vite": "^5.3.2",
    "vitepress": "^1.2.2",
    "vitest": "^1.6.0",
    "volar-service-vetur": "^0.0.54"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://codigo.conahcyt.mx/sisdai/sisdai-componentes"
  },
  "bugs": {
    "url": "https://codigo.conahcyt.mx/sisdai/sisdai-componentes/issues"
  },
  "homepage": "https://codigo.conahcyt.mx/sisdai/sisdai-componentes#readme"
}
